if((EC == 1) || (EC == 2) || (EC == 3) ||([PROCESS_TABLE + ([SYSTEM_STATUS_TABLE + 1]*16) + 13] >= ((PTLR * 512) - 1)))	then

	if(EC == 1)	then
		print "Ilgl_Ins"; 

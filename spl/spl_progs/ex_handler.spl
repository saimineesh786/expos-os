if((EC == 0) && (EPN >= 4) && (EPN <= 7))	then

	alias disk_block_num R1;
	disk_block_num = [DISK_MAP_TABLE + ([SYSTEM_STATUS_TABLE + 1] * 10)  + EPN];
	multipush(R1,EIP,EC,EPN,EMA);

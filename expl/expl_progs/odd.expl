int main(){
    decl
    	str fname; 
        int odd;
        int rt;
    enddecl
    begin
        rt=exposcall("Write",-2,"odd");
        odd=1;
        while(odd<=100) do
            rt=exposcall("Write",-2,odd);
            odd=odd+2;
            if(odd == 51)	then
            	rt = exposcall("Read",-1,fname);
            	rt = exposcall("Exec",fname);
            endif;
        endwhile;
        return 0;
    end
}
